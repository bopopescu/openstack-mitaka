#, python-format
msgid "\t%s"
msgstr "\t%s"

#, python-format
msgid "%(attribute)s have to be string"
msgstr "%(attribute)s debe ser una cadena"

#, python-format
msgid "%(attribute)s is required"
msgstr "%(attribute)s es obligatorio"

#, python-format
msgid "%(attribute)s may not be longer than %(length)i"
msgstr "%(attribute)s no puede ser más largo que %(length)i"

#, python-format
msgid "%(attribute)s may not be shorter than %(length)i"
msgstr "%(attribute)s no puede ser más corto que %(length)i "

